  "private": true,
  "dependencies": {
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "jquery": "^3.3.1",
    "morgan": "^1.9.1",
    "node-craigslist": "^2.0.0",
    "node-fetch": "^2.3.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
