  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "node-sass-chokidar": "^1.3.4",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5"
  }
}
